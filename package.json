  "repository": {
    "type": "git",
    "url": "git+https://github.com/iftakharul28/iftakhar-editor.git"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@types/node": "^20.12.7",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "postcss": "^8.4.38",
    "prop-types": "^15.8.1",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19",
    "rollup": "^4.16.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "style-loader": "^4.0.0",
    "tinymce": "^7.3.0",
    "tsc-hooks": "^1.1.2",
    "typescript": "^5.4.5"
  },
  "peerDependencies": {
    "prop-types": "^15.8.1",
    "react": "^18 || ^19",
    "react-dom": "^18 || ^19"
  },
  "dependencies": {
    "eslint-scope": "^5.1.1",
    "tslib": "^2.8.1"
  },
  "bugs": {
    "url": "https://github.com/iftakharul28/iftakhar-editor/issues"
  },
  "homepage": "https://github.com/iftakharul28/iftakhar-editor#readme"
